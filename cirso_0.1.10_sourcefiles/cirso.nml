item (FEAT_OBJECTS, item_res_vehicles_r) {
    property {
        class:                  "FACR";
        classname:              string(STR_FACR);
        name:                   string(STR_RES_VEHICLES);
        climates_available:     ALL_CLIMATES;
        size:                   [1,1];
        build_cost_multiplier:  1;
        remove_cost_multiplier: 1;
        introduction_date:      date(1801,1,1);
        end_of_life_date:       0xFFFFFFFF;
		object_flags:           bitmask(OBJ_FLAG_ANYTHING_REMOVE, OBJ_FLAG_REMOVE_IS_INCOME, OBJ_FLAG_ALLOW_BRIDGE);
        height:                 1;
        num_views:              4;
    }
    graphics {
        default:            	switch_res_vehicles_random_select;
        purchase:           	switch_res_vehicles_random_select_purchase;
        additional_text:		string(STR_RES_VEHICLES_PURCHASE);
    }
}


